<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/TESTER.styles.css" asp-append-version="true" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Caveat:wght@400..700&family=Open+Sans:ital,wght@0,300..800;1,300..800&family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap" rel="site.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200&icon_names=mail" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@24,400,0,0&icon_names=editor_choice" />
    <link  ref="https://fonts.googleapis.com/css?family=Montserrat:400,700|Source+Sans+Pro:300,400" rel="stylesheet">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200&icon_names=loyalty" />
</head>

<body>
    <div class="landing-page">
        <header>
            <div class="container">
                <a href="#" class="logo"> <img src="~/css/IMG-20250228-WA0043.jpg" alt="Alternate Text" /></a>


                <ul class="links" style="font-family:Arial; font-weight:500; color: #BB271A">

                    <li style=""><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>

                    <li><a asp-area="" asp-controller="Gallery" asp-action="Index">Gallery</a></li>

                    <li><a asp-area="" asp-controller="Plans" asp-action="Index">Plans</a></li>

                    <li><a asp-area="" asp-controller="Contact" asp-action="Index">Contact Us</a></li>

                    <li><a asp-area="" asp-controller="GetQuote" asp-action="Create">Get A Quote</a></li>
                </ul>
            </div>
        </header>
    </div>
    <div>
        @RenderBody()
    </div>

   


</body>
</html>
